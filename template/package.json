{
  "name": "Blade",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "reset": "react-native start --reset-cache",
    "ios:dev": "react-native run-ios --scheme 'BladeDev' --simulator='iPhone 13 mini'",
    "ios:stg": "react-native run-ios --scheme 'BladeStg' --simulator='iPhone 13 mini'",
