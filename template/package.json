    "@rneui/base": "^0.0.0-edge.2",
    "@rneui/themed": "^0.0.0-edge.2",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "lottie-ios": "3.4.0",
    "lottie-react-native": "^5.1.4",
    "plop": "^3.1.1",
